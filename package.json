{
  "name": "aws-cdk8s",
  "version": "0.1.0",
  "bin": {
    "aws-cdk8s": "bin/aws-cdk8s.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk8s": "cdk8s",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy",
    "cdk8s:synth": "cdk8s synth",
    "cdk8s:deploy": "cdk8s synth | kubectl apply -f -",
    "synth": "npm run cdk:synth && npm run cdk8s:synth",
    "deploy": "npm run cdk:deploy && npm run cdk8s:deploy"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.11.18",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.64.0",
    "cdk8s-cli": "^2.2.10",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.64.0",
    "cdk8s": "^2.7.54",
    "cdk8s-plus-25": "^2.6.18",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
